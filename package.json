{
  "name": "@penxle/web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^18 || ^20",
    "pnpm": "^8.6.0"
  },
  "scripts": {
    "bootstrap": "pnpm install && husky install",
    "dev": "doppler run --command 'vite dev'",
    "build": "vite build",
    "sync": "pnpm run codegen",
    "codegen": "pnpm run codegen:kit && pnpm run codegen:db && pnpm run codegen:gql",
    "codegen:kit": "doppler run --command 'pnpm exec svelte-kit sync'",
    "codegen:db": "prisma generate",
    "codegen:gql": "houdini generate --pull-schema",
    "db:migrate": "doppler run --command 'pnpm exec prisma migrate dev'",
    "db:migrate:create": "doppler run --command 'pnpm exec prisma migrate dev --create-only'",
    "lint": "eslint --max-warnings=0 .",
    "format": "eslint --fix .",
    "check": "svelte-check --fail-on-warnings",
    "test": "playwright test",
    "vercel-build": "svelte-kit sync && prisma generate && pnpm run build && prisma migrate deploy"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.354.0",
    "@aws-sdk/s3-request-presigner": "^3.354.0",
    "@envelop/core": "^4.0.0",
    "@envelop/types": "^4.0.0",
    "@felte/core": "^1.4.0",
    "@felte/validator-zod": "^1.0.16",
    "@floating-ui/dom": "^1.4.1",
    "@iconify-json/lucide": "^1.1.102",
    "@iconify-json/ri": "^1.1.9",
    "@iconify/utils": "^2.1.6",
    "@playwright/test": "^1.35.1",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "^3.52.0",
    "@pothos/plugin-scope-auth": "^3.19.2",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@pothos/plugin-validation": "^3.9.2",
    "@prisma/client": "^4.15.0",
    "@sentry/sveltekit": "^7.55.2",
    "@svelte-kits/store": "^0.1.1",
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.20.4",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-bold": "^2.0.3",
    "@tiptap/extension-document": "^2.0.3",
    "@tiptap/extension-hard-break": "^2.0.3",
    "@tiptap/extension-heading": "^2.0.3",
    "@tiptap/extension-history": "^2.0.3",
    "@tiptap/extension-italic": "^2.0.3",
    "@tiptap/extension-paragraph": "^2.0.3",
    "@tiptap/extension-placeholder": "^2.0.3",
    "@tiptap/extension-strike": "^2.0.3",
    "@tiptap/extension-text": "^2.0.3",
    "@tiptap/extension-text-align": "^2.0.3",
    "@tiptap/extension-typography": "^2.0.3",
    "@tiptap/extension-underline": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@unocss/eslint-plugin": "^0.53.1",
    "@vercel/analytics": "^1.0.1",
    "@vercel/edge-config": "^0.2.1",
    "argon2": "^0.30.3",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.8",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "felte": "^1.2.10",
    "gitmoji-cli": "^8.3.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.2",
    "graphql-yoga": "^4.0.1",
    "houdini": "^1.2.5",
    "houdini-svelte": "^1.2.5",
    "husky": "^8.0.3",
    "jose": "^4.14.4",
    "lint-staged": "^13.2.2",
    "nanoid": "^4.0.2",
    "network-information-types": "^0.1.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prisma": "^4.15.0",
    "query-string": "^8.1.0",
    "radash": "^10.9.0",
    "sass": "^1.63.4",
    "svelte": "4.0.0-next.2",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "svgo": "^3.0.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "vite": "^4.3.9",
    "web-vitals": "^3.3.2",
    "zod": "^3.21.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "svelte": "4"
      }
    }
  }
}
